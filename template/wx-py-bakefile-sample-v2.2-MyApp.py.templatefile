import wx
from MyFrame import MyFrame

class MyApp(wx.App):
	def OnInit(self):
		self.frame = MyFrame(None, title="The Main Frame")
		self.SetTopWindow(self.frame)
		self.frame.Show()


		return True
	# end def OnInit
# end class MyApp
